#:import CardTransition kivy.uix.screenmanager.CardTransition
#:import F kivy.factory.Factory

<Principal>:
    canvas:
        Color:
            rgba: (88/255),(2/255),(84/255),(35/100)
        Rectangle:
            pos: self.pos
            size: self.size

<Login>
    canvas:
        Color:
            rgb: 1,1,1
        Rectangle:
            pos: self.pos
            size: self.size
            source: '1314.png'
    BoxLayout:
        size_hint_x:0.2
    BoxLayout:
        size_hint_x:0.6
        BoxLayout:
            orientation: 'vertical'

            BoxLayout:
                size_hint_y:0.2

            TextInput:
                id: P1_input_usuario
                size_hint_y:0.05
                hint_text: 'Usuario'

            BoxLayout:
                size_hint_y:0.02

            TextInput:
                id: P1_input_contra
                size_hint_y:0.05
                hint_text: 'Contrasena'
                password: ''


            BoxLayout:
                orientation: 'horizontal'
                size_hint_y:0.05
                Label:
                    text: 'Ver contrasena'
                Switch:
                    id: contra_on_off
                    on_touch_down: if self.active is False: P1_input_contra.password = True
                    on_touch_down: if self.active is True: P1_input_contra.password = False



            BoxLayout:
                orientation: 'horizontal'
                size_hint_y:0.05
                Button:
                    background_color: (108/255),(70/255),(240/255),1
                    text: 'Registro'
                    on_press:
                        root.Principal.transition.direction = 'down'
                        root.Principal.transition.duration = 0.7
                        root.Principal.goto_registro()
                Button:
                    background_color: (70/255),(240/255),(101/255),1
                    text: 'Ingresar'
                    on_press:
                        root.Principal.transition.direction = 'left'
                        root.Principal.transition.duration = 0.7
                        root.inicio_sesion()




            BoxLayout:
                size_hint_y:0.4

    BoxLayout:
        size_hint_x:0.2

<Registro>
    canvas:
        Color:
            rgb: 1,1,1
        Rectangle:
            source: '1314-regis.png'
            pos: self.pos
            size: self.size

    BoxLayout:
        size_hint_x:0.2
    BoxLayout:
        size_hint_x:0.6
        BoxLayout:
            orientation: 'vertical'

            BoxLayout:
                size_hint_y:0.2

            TextInput:
                id: P3_input_usuario
                size_hint_y:0.05
                hint_text: 'Usuario'

            BoxLayout:
                size_hint_y:0.02

            TextInput:
                id: P3_input_contra
                size_hint_y:0.05
                hint_text: 'Contrasena'
                password: ''


            BoxLayout:
                orientation: 'horizontal'
                size_hint_y:0.05
                Label:
                    text: 'Ver contrasena'
                Switch:
                    id: contra_on_off
                    on_touch_down: if self.active is False: P3_input_contra.password = True
                    on_touch_down: if self.active is True: P3_input_contra.password = False



            BoxLayout:
                orientation: 'horizontal'
                size_hint_y:0.05
                Button:
                    text: 'Registrarse'
                    on_press: root.registro_nuevo_usuario()
                Button:
                    background_color: (105/255),(240/255),(125/255),1
                    text: 'Volver'
                    on_press:
                        root.Principal.transition.direction = 'up'
                        root.Principal.transition.duration = 0.7
                        root.Principal.goto_inicio()


            BoxLayout:
                size_hint_y:0.4

    BoxLayout:
        size_hint_x:0.2

<Seleccion_ciudad>
    padding: 10
    orientation: 'vertical'
    BoxLayout:
        padding:10
        orientation: 'horizontal'
        Button:
            text: 'Barranquilla'
            id: barranquilla
            on_press: root.ciudadbar()
            on_press: barranquilla.background_color = (0/255),(255/255),(255/255),1
            on_press: cartagena.background_color = (1,1,1,1)
            on_press: bogota.background_color = (1,1,1,1)
            on_press: medellin.background_color = (1,1,1,1)
        Button:
            text: 'Bogota'
            id: bogota
            on_press: root.ciudadbog()
            on_press: bogota.background_color = (0/255),(255/255),(255/255),1
            on_press: barranquilla.background_color = (1,1,1,1)
            on_press: cartagena.background_color = (1,1,1,1)
            on_press: medellin.background_color = (1,1,1,1)

    BoxLayout:
        padding:10
        orientation: 'horizontal'
        Button:
            id: cartagena
            text: 'Cartagena'
            on_press: root.ciudadcar()
            on_press: cartagena.background_color = (0/255),(255/255),(255/255),1
            on_press: barranquilla.background_color = (1,1,1,1)
            on_press: bogota.background_color = (1,1,1,1)
            on_press: medellin.background_color = (1,1,1,1)
        Button:
            id: medellin
            text: 'Medellin'
            on_press: root.ciudadmed()
            on_press: medellin.background_color = (0/255),(255/255),(255/255),1
            on_press: barranquilla.background_color = (1,1,1,1)
            on_press: bogota.background_color = (1,1,1,1)
            on_press: cartagena.background_color = (1,1,1,1)
    BoxLayout:
        orientation: 'vertical'
        size_hint_y: 8
        Spinner:
            id: spinner_inicio
            text: 'Ninguno'
            values:
            on_text:
                if self.text != 'Ninguno': P2_button_siguiente.disabled = False
                if self.text == 'Ninguno': P2_button_siguiente.disabled = True
        BoxLayout:
            size_hint_y:8
        Button:
            id: P2_button_siguiente
            text: 'Siguiente'
            disabled: True
            on_press:
                root.Principal.transition.direction = 'left'
                root.Principal.transition.duration = 0.5
                root.Principal.goto_comidas()
                root.cidad_elegida(spinner_inicio.text,spinner_inicio.values)



<Restaurantes>
    orientation: 'vertical'
    Spinner:
        id: spinner_incio
        text: 'Ninguno'
        values: root.ListaSedes
        on_text:
            if self.text != 'Ninguno': P2_button_siguiente.disabled = False
            if self.text == 'Ninguno': P2_button_siguiente.disabled = True
    BoxLayout:
